# RevenueCat Configuration
# Get your API key from https://app.revenuecat.com/settings/api-keys
VITE_REVENUECAT_API_KEY=pk_test_your_api_key_here

# Apple App Store Configuration
# Get from https://appstoreconnect.apple.com
APPLE_SHARED_SECRET=your_apple_shared_secret_here

# Google Play Configuration
# Get from Google Play Console
GOOGLE_PACKAGE_NAME=com.gatofit.app
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_SERVICE_ACCOUNT_JSON='{"type":"service_account","project_id":"your_project","private_key":"...","client_email":"..."}'

# Environment
ENVIRONMENT=development
